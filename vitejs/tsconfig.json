{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "allowJs": false,
        "skipLibCheck": true,
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "paths": {
            "_App/*": [
                "./src/App/*"
            ],
            "_processed/*": [
                "./src/processed/*"
            ],
            "_pages/*": [
                "./src/pages/*"
            ],
            "_widgets/*": [
                "./src/widgets/*"
            ],
            "_features/*": [
                "./src/features/*"
            ],
            "_entities/*": [
                "./src/entities/*"
            ],
            "_shared/*": [
                "./src/shared/*"
            ],
            "_assets/*": [
                "./src/assets/*"
            ],
            "_styles/*": [
                "./src/styles/*"
            ],
        }
    },
    "include": [
        "src"
    ],
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}
